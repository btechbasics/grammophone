{
  "private": true,
  "version": "0.0.0",
  "name": "grammophone",
  "browserslist": "> 0.5%, last 2 versions",
  "type": "module",
  "source": [
    "src/index.html"
  ],
  "dependencies": {
    "@lezer/lr": "^1.3.3",
    "preact": "^10.13.0"
  },
  "devDependencies": {
    "@lezer/generator": "^1.2.2",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jshint": "^2.13.6",
    "mocha": "^10.2.0",
    "parcel": "^2.8.3"
  },
  "scripts": {
    "generate-lezer-parser": "lezer-generator --output src/parser/rules.js src/parser/rules.grammar",
    "lint": "eslint src test",
    "test": "mocha --recursive --ignore \"test/fixtures/*\"",
    "build": "parcel build --no-autoinstall --no-cache",
    "serve": "parcel serve --no-autoinstall --no-cache"
